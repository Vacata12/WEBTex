{
  "name": "webtex",
  "version": "1.0.0",
  "description": "WEBTex Cloud Drive",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "start": "node dist/server.js",
    "dev": "nodemon",
    "init-db": "ts-node --transpile-only src/db/dbInit.ts",
    "clean": "rm -rf node_modules && rm -f package-lock.json && npm cache clean --force",
    "reinstall": "npm run clean && npm install"
  },
  "dependencies": {
    "@types/multer": "^1.4.12",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "file-type": "^21.0.0",
    "mongoose": "^7.5.3",
    "multer": "^1.4.5-lts.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.18",
    "@types/node": "^18.18.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
